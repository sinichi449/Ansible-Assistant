            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Here is $folder and its files."
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.example.commands.LSL",
                                    "params": {
                                        "folder": "$folder"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
